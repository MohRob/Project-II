@model IEnumerable<Ibn_Sina_Diagnostic_Center.Models.DrTable>

@{
    ViewBag.Title = "FindDr";
}

<h2>FindDr</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.About)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AppointmentDetails)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Chamber_details)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.About)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AppointmentDetails)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Chamber_details)
        </td>
        <td>
            @Html.ActionLink("Appointment", "Add", new { id=item.Id,name=item.Name,apointmentDetails=item.AppointmentDetails }) |

        </td>
    </tr>
}

</table>
